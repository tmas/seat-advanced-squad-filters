{
    "name": "tmas/advanced-squad-filters",
    "description": "A draft refactor of SeAT's filter system, built as a package to allow immediate use",
    "type": "seat-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "tmas"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "eveseat/services": "^5.0",
        "eveseat/eveapi": "^5.0",
        "eveseat/web": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Tmas\\AdvancedSquadFilters\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tmas\\AdvancedSquadFilters\\AdvancedSquadFiltersServiceProvider"
            ]
        }
    }
}
